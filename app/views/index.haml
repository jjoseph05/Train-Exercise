.container
	.train_listings
	%table
		%thead
			%th TrainLine
			%th RouteName
			%th RunNumber
			%th OperatorID
		%tbody
			- @listings.each do |listing|
				%tr
				%td= listing.train_line
				%td= listing.route_name
				%td= listing.run_number
				%td= listing.operator_id
	%h2 Upload
	%form{:action=>"/upload",:method=>"post",:enctype=>"multipart/form-data"}
		%input{:type=>"file",:name=>"file"}
		%input{:type=>"submit",:value=>"Upload"}
	